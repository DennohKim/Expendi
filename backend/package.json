{
  "name": "expendi-analytics-backend",
  "version": "1.0.0",
  "description": "Backend analytics service for Expendi DApp",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "npm run build && npx prisma migrate deploy && npx prisma generate && node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "sync:manual": "tsx scripts/manual-sync.ts",
    "sync:base": "tsx scripts/manual-sync.ts chain base",
    "sync:all": "tsx scripts/manual-sync.ts all-chains",
    "sync:status": "tsx scripts/manual-sync.ts status",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"No linter configured\" && exit 0"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "prisma": "^5.7.1"
  }
}